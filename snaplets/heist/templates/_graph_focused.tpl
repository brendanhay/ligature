<div class="item ${graphActive}" style="height: ${height}px">
  <div class="container">
    <div class="carousel-caption">
      <h1><graphName /></h1>
      <p class="lead"><graphDesc /></p>
    </div>
  </div>
  <img class="graph-image" style="height: ${height}px; width: 100%" data-resize="true" data-src="${graphUrl}">
</div>
